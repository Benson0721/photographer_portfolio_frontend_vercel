{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "photographer-portfolio-frontend-vercel.vercel.app"
        }
      ],
      "destination": "/index.html"
    }
  ]
}
