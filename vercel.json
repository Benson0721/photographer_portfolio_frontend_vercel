{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "text/html"
        }
      ],
      "destination": "/index.html"
    }
  ],
  "redirects": [],
  "headers": [],
  "fallback": {
    "source": "/(.*)",
    "destination": "/index.html"
  }
}
